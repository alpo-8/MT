      "auto_delete": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "failed.books",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "failed.exchange",
      "vhost": "/",
      "destination": "failed.books",
      "destination_type": "queue",
      "routing_key": "book",
      "arguments": {}
